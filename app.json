{
  "expo": {
    "name": "Prime_Cuts",
    "slug": "Prime_Cuts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "primecuts",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],

       [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DancingScript-Bold.ttf",
            "./assets/fonts/DancingScript-Medium.ttf",
            "./assets/fonts/DancingScript-Regular.ttf",
            "./assets/fonts/DancingScript-SemiBold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "DancingScript",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/DancingScript-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/DancingScript-Medium.ttf",
                    "weight": 600
                  },
                  {
                    "path": ".assets/fonts/DancingScript-Regular.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/DancingScript-SemiBold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
